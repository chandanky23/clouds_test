{
  "name": "test-aiven",
  "version": "1.0.0",
  "description": "A test project",
  "scripts": {
    "install-server": "cd server && python3 -m venv venv && venv/bin/pip3 install -r requirements.txt",
    "server": "cd server && venv/bin/flask run --no-debugger",
    "test-server": "cd server && venv/bin/python3 test_app.py",
    "test": "concurrently \"npm run install-server\" \"npm run server\" \"npm run test-server\"",
    "app": "webpack-dev-server",
    "dev": "concurrently \"npm run app\" \"npm run server\"",
    "build": "rm -rf build && NODE_ENV=production webpack",
    "test-app": "jest",
    "test-watch": "npm run test-app -- --watch",
    "prod": "concurrently NODE_ENV=production && npm i && npm run build && npm run install-server && npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chandanky23/clouds_test.git"
  },
  "keywords": [],
  "author": "chandan yadav",
  "license": "ISC",
  "homepage": "https://github.com/chandanky23/clouds_test#readme",
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/jest": "^26.0.14",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "^16.9.3",
    "@types/styled-components": "^5.1.3",
    "@types/testing-library__jest-dom": "^5.9.4",
    "@types/webpack": "^4.41.22",
    "@types/webpack-dev-server": "^3.11.0",
    "concurrently": "^5.3.0",
    "css-loader": "^4.3.0",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.4.2",
    "react-test-renderer": "^16.13.1",
    "source-map-loader": "^1.1.0",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
